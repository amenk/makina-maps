{
  "options": {
    "paths": {
      "root": "",
      "fonts": "fonts",
      "styles": "styles"
    },
    "domains": [
      "localhost:8080"
    ],
    "formatQuality": {
      "jpeg": 80,
      "webp": 90
    },
    "maxScaleFactor": 3,
    "maxSize": 2048,
    "pbfAlias": "pbf",
    "serveAllFonts": false,
    "serveAllStyles": false,
    "serveStaticMaps": true,
    "tileMargin": 10
  },
  "styles": {
    "bright": {
      "style": "osm-bright-gl-style/style-local.json"
    },
    "basic": {
      "serve_rendered": false,
      "style": "klokantech-basic-gl-style/style-local.json"
    },
    "cassini": {
      "serve_rendered": false,
      "style": "cassini-gl-style/style-local.json"
    }
  },
  "data": {
    "v3": {
      "remote_tilejson": "http://postserve:8090/",
      "tilejson": {
        "tiles": [
          "http://nginx/data/v3/{z}/{x}/{y}.pbf"
        ]
      }
    }
  }
}
